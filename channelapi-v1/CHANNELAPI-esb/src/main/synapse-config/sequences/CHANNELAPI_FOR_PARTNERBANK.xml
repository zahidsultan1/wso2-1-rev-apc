	<filter regex="yes" source="$ctx:loan">
		<then>
			<payloadFactory media-type="json">
				<format>{ "error": { "errorCode": "$1", "description": "$2", "message": "", "transaction": { "transactionId": "$3" }, "STATUS": "$4" } }</format>
