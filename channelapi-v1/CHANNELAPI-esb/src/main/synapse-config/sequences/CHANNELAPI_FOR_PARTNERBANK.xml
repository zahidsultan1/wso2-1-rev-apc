							<property action="remove" name="Access-Control-Allow-Origin" scope="transport"/>
							<property action="remove" name="TRANSPORT_HEADERS" scope="axis2"/>
							<property action="remove" name="EXCESS_TRANSPORT_HEADERS" scope="axis2"/>
