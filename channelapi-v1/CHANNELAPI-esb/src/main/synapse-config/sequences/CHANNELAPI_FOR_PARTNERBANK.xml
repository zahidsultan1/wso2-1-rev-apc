									<arg value="411"/>
									<arg value="Decimal value not allowed"/>
									<arg evaluator="xml" expression="$ctx:TRANSACTION_ID"/>
									<arg value="KO"/>
								</args>
							</payloadFactory>
							<property name="SC_ACCEPTED" scope="axis2" type="STRING" value="false"/>
							<property name="HTTP_SC" scope="axis2" type="STRING" value="200"/>
