<?xml version="1.0" encoding="UTF-8"?>
<sequence name="APC_MSA_HUB_TRANSACTION_STATUS" xmlns="http://ws.apache.org/ns/synapse">
	<property expression="$ctx:ServiceUser" name="SERVICE_USER" xmlns:ns="http://org.apache.synapse/xsd"/>
	<property expression="$ctx:DBStatus" name="STATUS" xmlns:ns="http://org.apache.synapse/xsd"/>
	<property expression="$ctx:STATUSCODE" name="STATUS_CODE" xmlns:ns="http://org.apache.synapse/xsd"/>
	<property expression="$ctx:Description" name="DESCRIPTION" xmlns:ns="http://org.apache.synapse/xsd"/>
	<property expression="$ctx:External_Data" name="External_Data" xmlns:ns="http://org.apache.synapse/xsd"/>
	<clone continueParent="true" sequential="false">
		<target>
			<sequence>
				<dbreport>
					<connection>
						<pool>
							<dsName>jdbc/APCJendi</dsName>
						</pool>
					</connection>
					<statement>
						<sql><![CDATA[INSERT INTO APC_Generic.HUB_TRANSACTION_STATUS (ID, TRANSACTION_ID, SERVICE_USER, CHANNEL_NAME,  RETAILER_NO, AMOUNT, MSISDN, STATUS, STATUS_CODE, TIME_IN, TIME_OUT, DESCRIPTION, PARTNER_MSISDN, PARTNER_DAID, DA_CHECK, OPERATOR, TYPE, EXTERNAL_DATA ) VALUES (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)]]></sql>
						<parameter expression="$ctx:ID" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:TRANSACTION_ID" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:SERVICE_USER" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:CHANNEL_NAME" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:RETAILER_NO" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:AMOUNT" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:MSISDN" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:STATUS" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:STATUS_CODE" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:TIME_IN" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:TIME_OUT" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:DESCRIPTION" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:PARTNER_MSISDN" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:PARTNER_DAID" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:DA_CHECK" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:OPERATOR" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:TYPE" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
						<parameter expression="$ctx:External_Data" type="VARCHAR" xmlns:ns="http://org.apache.synapse/xsd"/>
					</statement>
				</dbreport>
			</sequence>
		</target>
	</clone>
</sequence>
