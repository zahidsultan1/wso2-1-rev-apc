<?xml version="1.0" encoding="UTF-8"?>
<api context="/ChannelAPI" name="GENERIC_CHECKMSISDN" xmlns="http://ws.apache.org/ns/synapse">
	<resource methods="GET" uri-template="/checkmsisdn/{msisdn}/{transaction_id}">
		<inSequence>
			<property expression="$trp:APPNAME" name="APP_NAME" scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
			<property expression="$trp:APINAME" name="API_NAME" scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
			<property expression="$ctx:uri.var.msisdn" name="msisdn" scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
			<property expression="$ctx:uri.var.msisdn" name="uri.var.msisdn" scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
			<property expression="$ctx:uri.var.msisdn" name="MSISDN" scope="default" type="STRING" xmlns:ns="http://org.apache.synapse/xsd"/>
